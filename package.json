{
  "name": "vava",
  "version": "3.6.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "css-template": "node-sass --output-style expanded src/themes/theme-template.scss static/css/theme-template.css && npm run css-minify",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --output static/css/theme-template.min.css static/css/theme-template.css",
    "svgo": "svgo -f src/icons/svgs --config=./src/icons/svgo.yml",
    "svg_imgs": "svgo -f public/static/svgs --config=./public/static/svgs/svgo.yml"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^2.1.0",
    "axios": "^0.19.0",
    "clipboard": "^2.0.4",
    "css-color-function": "^1.3.3",
    "driver.js": "^0.9.7",
    "echarts": "^4.2.1",
    "element-ui": "2.10.1",
    "file-saver": "^2.0.2",
    "highlight.js": "^10.4.1",
    "js-cookie": "^2.2.0",
    "mockjs": "^1.0.1-beta3",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "screenfull": "^4.2.0",
    "sortablejs": "^1.10.0-rc3",
    "vue": "^2.6.10",
    "vue-ba": "^1.2.5",
    "vue-count-to": "^1.0.13",
    "vue-i18n": "^8.12.0",
    "vue-meta": "^2.0.5",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1",
    "xlsx": "^0.14.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/register": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-e2e-cypress": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-unit-mocha": "^3.9.0",
    "@vue/cli-service": "^3.9.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.2",
    "clean-css-cli": "^4.3.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "lint-staged": "^9.2.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "script-loader": "^0.7.2",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.2.2",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
